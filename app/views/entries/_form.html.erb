<%= form_for @entry do |f| %>
  <%= f.label :author %><br>
  <%= f.text_field :author %><br>

  <%= f.label :photo_url %><br>
  <%= f.text_field :photo_url %><br>

  <%= f.label :date_taken %><br>
  <%= f.date_field :date_taken %><br>

  <%= f.label :caption %><br>
  <%= f.text_area :caption %><br>

  <%= f.submit %>
<% end %>