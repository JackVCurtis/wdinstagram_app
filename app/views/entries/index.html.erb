<h1>All Photos</h1>

<table>
  <th>Author</th>
  <th>Date Taken</th>
<% @entries.each do |entry| %>  
  <tr>
    <td><%= entry.author %></td>
    <td><%= entry.date_taken.to_formatted_s(:long) %></td>
    <td><%= link_to "View", entry %></td>
    <td><%= link_to "Edit", edit_entry_path(entry) %></td
  </tr>
<% end %>
</table>